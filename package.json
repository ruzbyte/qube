{
  "name": "qube-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -n web,api -c blue,magenta \"bun --filter ./apps/web dev\" \"bun --filter ./apps/api dev\"",
    "build": "bun --filter '*' build"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
